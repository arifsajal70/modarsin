{
  "name": "modarsin",
  "version": "1.0.0",
  "description": "Modarsin Portal",
  "main": "src/app/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-all": "electron-builder -mw"
  },
  "build": {
    "appId": "com.modarsin.app",
    "productName": "Modarsin",
    "mac": {
