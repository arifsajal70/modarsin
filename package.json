    },
    "win": {
      "target": "portable",
      "icon": "src/assets/images/logo.png"
    }
  },
  "author": "Arif Sajal",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {}
